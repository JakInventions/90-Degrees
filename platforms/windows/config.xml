<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jakinventions.NinetyDegrees" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <name>Ninety Degrees</name>
    <description>
        A minimalist hypercasual game.
    </description>
    <author email="support@jakinventions.com" href="https://jakinventions.com">
        Jak Inventions
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
